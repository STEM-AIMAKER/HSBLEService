{
    "name": "hsbleservice",
    "version": "1.0.1",
    "description": "HANSHIN bluetooth ble library for microbit",
    "license": "MIT",
    "dependencies": {
        "core": "*",
        "bluetooth": "*"
    },
    "files": [
        "README.md",
        "main.ts",
        "HSMicroBitUARTService.cpp",
        "HSMicroBitUARTService.h",
        "hsble.cpp",
        "shims.d.ts",
        "enums.d.ts"
    ],
    "public": true,
    "supportedTargets": [
        "microbit"
    ],
    "preferredEditor": "tsprj",
    "yotta": {
        "config": {
            "microbit-dal": {
                "bluetooth": {
                    "open": 1,
                    "whitelist": 0,
                    "security_level": null
                }
            }
        }
    }
}
